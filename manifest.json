{
  "manifest_version": 2,

  "name": "Urban Dictionary QuickSearch",
  "description": "This extension allows the user to search selected words on Urban Dictionary",
  "version": "1.0",
  "minimum_chrome_version": "6",

  "browser_action": {
    "default_title": "Search Urban Dictionary",
    "default_popup": "popup.html"
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "contextMenus",
    "tabs"
  ]
}

